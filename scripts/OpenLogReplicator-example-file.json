{
  "version": "1.9.0",
  "memory": {
    "min-mb": 64,
    "max-mb": 1024
  },
  "source": [
    {
      "alias": "SOURCE",
      "name": "DBNAME",
      "reader": {
        "type": "online",
        "user": "user1",
        "password": "Password1",
        "server": "//host:1521/SERVICE"
      },
      "format": {
        "type": "json"
      },
      "filter": {
        "table": [
          {"owner": "OWNER1", "table": "TABLENAME1", "key": "COL1,COL2,COL3"},
          {"owner": "OWNER1", "table": "TABLENAME2"},
          {"owner": "OWNER2", "table": "TAB.*"}
        ]
      }
    }
  ],
  "target": [
    {
      "alias": "FILE",
      "source": "SOURCE",
      "writer": {
        "type": "file",
        "output": "output-%t.json",
        "new-line": 1,
        "max-file-size": 1073741824,
        "append": 0
      }
    }
  ]
}
