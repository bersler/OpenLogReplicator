{
  "version": "1.6.1",
  "source": [
    {
      "alias": "S1",
      "name": "O112A",
      "reader": {
        "type": "online",
        "user": "user1",
        "password": "Password1",
        "server": "//host:1521/SERVICE"
      },
      "format": {
        "type": "json"
      },
      "memory": {
        "min-mb": 64,
        "max-mb": 1024
      },
