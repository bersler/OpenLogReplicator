          {"owner": "OWNER2", "table": "TAB.*"}
        ]
      }
    }
  ],
  "target": [
    {
      "alias": "K1",
      "source": "S1",
      "writer": {
        "type": "kafka",
        "topic": "O112A",
        "properties": {
          "bootstrap.servers": "localhost:9092"
        }
      }
    }
  ]
}
